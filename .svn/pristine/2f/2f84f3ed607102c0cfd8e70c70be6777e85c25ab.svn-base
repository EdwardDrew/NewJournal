<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/lv_search_result_bg" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/rl_touxiang"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:padding="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="头像"
                    android:textColor="@color/back"
                    android:textSize="@dimen/default_textSize" >
                </TextView>

                <com.ipbase.widget.CircleImage
                    android:id="@+id/iv_touxiang"
                    android:layout_width="52dp"
                    android:layout_height="52dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/icon_man_black" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:background="@color/divider" />

            <RelativeLayout
                android:id="@+id/rl_account"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:padding="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="账号"
                    android:textColor="@color/back"
                    android:textSize="@dimen/default_textSize" >
                </TextView>

                <TextView
                    android:id="@+id/tv_account"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30dp"
                    android:text="18814090301"
                    android:textColor="@color/gray6"
                    android:textSize="@dimen/default_textSize" >
                </TextView>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:background="@color/divider" />

            <RelativeLayout
                android:id="@+id/rl_nickname"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:padding="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="昵称"
                    android:textColor="@color/back"
                    android:textSize="@dimen/default_textSize" >
                </TextView>

                <TextView
                    android:id="@+id/tv_nickname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30dp"
                    android:text="名字"
                    android:textColor="@color/gray6"
                    android:textSize="@dimen/default_textSize" >
                </TextView>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:background="@color/divider" />

            <RelativeLayout
                android:id="@+id/rl_bind_phone"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:padding="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="绑定手机"
                    android:textColor="@color/back"
                    android:textSize="@dimen/default_textSize" >
                </TextView>

                <TextView
                    android:id="@+id/tv_bind_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30dp"
                    android:text="18814090301"
                    android:textColor="@color/gray6"
                    android:textSize="@dimen/default_textSize" >
                </TextView>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:background="@color/divider" />

            <RelativeLayout
                android:id="@+id/rl_bind_mail"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:padding="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="绑定邮箱"
                    android:textColor="@color/back"
                    android:textSize="@dimen/default_textSize" >
                </TextView>

                <TextView
                    android:id="@+id/tv_bind_mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30dp"
                    android:text="2939344954@qq.com"
                    android:textColor="@color/gray6"
                    android:textSize="@dimen/default_textSize" >
                </TextView>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:background="@color/divider" />

            <RelativeLayout
                android:id="@+id/rl_WeChatOrAlipay"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:padding="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="微信号/支付宝"
                    android:textColor="@color/back"
                    android:textSize="@dimen/default_textSize" >
                </TextView>

                <TextView
                    android:id="@+id/tv_WeChatOrAlipay"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="30dp"
                    android:text="13738390489"
                    android:textColor="@color/gray6"
                    android:textSize="@dimen/default_textSize" >
                </TextView>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:background="@color/divider" />
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/ll_icon"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#000000"
        android:visibility="gone" >

        <com.ipbase.widget.IconView
            android:id="@+id/icon_big"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:contentDescription="@string/app_name"
            android:scaleType="matrix"
            android:visibility="gone" />
    </LinearLayout>

</FrameLayout>