package com.ipbase.bean;

public class YMWidgetBean
{
	private String mCreateTime;
	private String mId;
	private String mMonth;
	private String mTerm;
	private String mYear;

	public YMWidgetBean( String createTime, String id, String month,
			String term, String year )
	{
		this.mCreateTime = createTime;
		this.mId = id;
		this.mMonth = month;
		this.mTerm = term;
		this.mYear = year;
	}

	/**
	 * @return createTime
	 */
	public String getCreateTime()
	{
		return mCreateTime;
	}

	/**
	 * @return id
	 */
	public String getId()
	{
		return mId;
	}

	/**
	 * @return month
	 */
	public String getMonth()
	{
		return mMonth;
	}

	/**
	 * @return term
	 */
	public String getTerm()
	{
		return mTerm;
	}

	/**
	 * @return year
	 */
	public String getYear()
	{
		return mYear;
	}

	/**
	 * @param createTime
	 *            要设置的 createTime
	 */
	public void setCreateTime( String createTime )
	{
		this.mCreateTime = createTime;
	}

	/**
	 * @param id
	 *            要设置的 id
	 */
	public void setId( String id )
	{
		this.mId = id;
	}

	/**
	 * @param month
	 *            要设置的 month
	 */
	public void setMonth( String month )
	{
		this.mMonth = month;
	}

	/**
	 * @param term
	 *            要设置的 term
	 */
	public void setTerm( String term )
	{
		this.mTerm = term;
	}

	/**
	 * @param year
	 *            要设置的 year
	 */
	public void setYear( String year )
	{
		this.mYear = year;
	}
};